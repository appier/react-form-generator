{
  "name": "react-form-generator",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "jest:watch": "jest --watch",
    "gen": "babel-node ./src/generator/cli.js run",
    "gen:exampleA": "babel-node ./src/generator/cli.js run -c ../exampleAConfig.js",
    "jest:cover": "jest --coverage"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "coverageReporters": [
      "clover",
      "cobertura",
      "html"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/storybook": "^2.35.2",
    "@kadira/storyshots": "^2.1.0",
    "autoprefixer": "^6.7.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "jest": "^18.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  },
  "dependencies": {
