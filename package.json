    "fs-extra": "^2.0.0",
    "immutable": "^3.8.1",
    "json5": "^0.5.1",
    "mf-input-date-picker": "^1.6.1",
    "prettier": "^0.17.0",
    "react": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-select": "^1.0.0-rc.2",
    "yargs": "^6.6.0"
  }
